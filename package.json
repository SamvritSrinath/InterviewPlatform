{
  "name": "291y-interview-platform",
  "version": "0.1.0",
  "description": "Modern LeetCode-style platform with cheating detection for interview environments",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --filter=interview",
    "dev:interview": "turbo run dev --filter=interview",
    "dev:questions": "turbo run dev --filter=questions",
    "dev:all": "turbo run dev --parallel",
    "build": "turbo run build --filter=interview",
    "build:interview": "turbo run build --filter=interview",
    "build:questions": "turbo run build --filter=questions",
    "build:all": "turbo run build",
    "start": "turbo run start --filter=interview",
    "start:interview": "turbo run start --filter=interview",
    "start:questions": "turbo run start --filter=questions",
    "lint": "turbo run lint --filter=interview",
    "lint:interview": "turbo run lint --filter=interview",
    "lint:questions": "turbo run lint --filter=questions",
    "lint:all": "turbo run lint",
    "clean": "turbo run clean && rm -rf .turbo node_modules apps/*/node_modules packages/*/node_modules",
    "postinstall": ""
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0",
    "turbo": "^2.0.0",
    "tailwindcss": "^4",
    "@tailwindcss/postcss": "^4"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.3",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
