{
  "name": "291y-interview-platform",
  "version": "0.1.0",
  "description": "Modern LeetCode-style platform with cheating detection for interview environments",
  "private": true,
  "scripts": {
    "dev": "nx dev interview",
    "dev:interview": "nx dev interview",
    "dev:questions": "nx dev questions",
    "dev:all": "nx run-many --target=dev --projects=interview,questions --parallel=2",
    "build": "nx build interview",
    "build:interview": "nx build interview",
    "build:questions": "nx build questions",
    "build:all": "nx run-many --target=build --projects=interview,questions",
    "start": "nx start interview",
    "start:interview": "nx start interview",
    "start:questions": "nx start questions",
    "lint": "nx lint interview",
    "lint:interview": "nx lint interview",
    "lint:questions": "nx lint questions",
    "lint:all": "nx run-many --target=lint --projects=interview,questions",
    "affected:build": "nx affected --target=build",
    "affected:lint": "nx affected --target=lint",
    "affected:test": "nx affected --target=test",
    "graph": "nx graph",
    "clean": "nx reset && rm -rf .nx node_modules apps/*/node_modules packages/*/node_modules",
    "postinstall": ""
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0",
    "nx": "^20.1.0",
    "@nx/next": "^20.1.0",
    "@nx/js": "^20.1.0",
    "@nx/node": "^20.1.0",
    "@nx/workspace": "^20.1.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
